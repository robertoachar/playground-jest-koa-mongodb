{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "no-console": "error",
    "no-process-env": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ]
  }
}
